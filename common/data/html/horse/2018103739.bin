// $Id: get_horse_aptitude_box.html 154 2024-11-18 04:26:09Z ysimizu $
$(function(){
let horse_id;
const query = window.location.search.split('#')[0].split('?')[1];
if(undefined !== query){
const params = query.split('&');
for(let i=0; i < params.length; i++){
const val = params[i].split('=');
if('id' == val[0]){
horse_id = val[1];
}
}
}
if(undefined == horse_id){
const path = window.location.pathname.split('/');
for(let i=0; i < path.length; i++){
if('horse' == path[i] && ! path[i+1].match(/\.html/)){
horse_id = path[i+1];
}
}
}
$.get('https://db.netkeiba.com/social/ajax_horse_aptitude.html',
{
input: 'UTF-8',
output: 'json',
id: horse_id,
},
function(data){
if('OK' == data.status){
$('#horse_aptitude_box').html(data.data);
}
});
});
</script>
