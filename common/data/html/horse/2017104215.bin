<script>
// $Id: horse_ranking.html 23 2024-10-19 10:59:56Z ysimizu $
"use strict"
$(function(){
const urls = {
'access': ['https://db.netkeiba.com/ranking/ajax_db_access_rank.html','daily'],
'comment':['https://db.netkeiba.com/ranking/ajax_db_comment_rank.html','daily'],
'rating': ['https://db.netkeiba.com/ranking/ajax_db_rating_rank.html','all'],
};
let cur_tab = 'access';
let cur_sort = '';
$(document).on('click','[id^="adb-"]', function(){
const tab_id = $(this).prop('id').replace(/adb-/,'');
cur_tab = tab_id;
cur_sort = urls[cur_tab][1];
const param = {};
param['input'] = 'UTF-8';
param['sort'] = cur_sort;
ajax_common(urls[cur_tab][0], param, function(data){
if('OK' == data.status){
$('#out_html').empty();
$('#out_html').html(data.data);
}
});
reset_tab1();
$('#adb-'+cur_tab).addClass('active');
});
$('#adb-'+cur_tab).trigger('click');	// default action
//------------------------------------------------------------
function ajax_common(url, param, fn){
let _data = {};
_data.output = 'json';
for(var key in param){
_data[key] = param[key];
}
$.ajax({
type: "POST",
url: url,
data: _data,
dataType: _data.output,
success: function(json){
if(undefined !== fn){
fn(json);
}
},
error: function(XMLHttpRequest, textStatus, errorThrown) {
console.error('textStatus:',textStatus);
}
});
}
//------------------------------------------------------------
function reset_tab1(){
$('[id^="adb-"]').each(function(){
$(this).removeClass('active');
});
}
});
</script>
<span id="horse_bbs_ranking_box"></span>
<script>
// $Id: get_horse_bbs_ranking_box.html 149 2024-11-16 09:17:28Z ysimizu $
$(function(){
// https://db.netkeiba.com/horse/xxxxxxx/
const path = window.location.pathname.split('/');
let horse_id;
for(let i=0; i < path.length; i++){
if('horse' == path[i]){
horse_id = path[i+1];
}
}
if(undefined == horse_id){
// https://db.netkeiba.com/?pid=horse_detail&id=xxxxxxx
const query = window.location.search.split('#')[0].split('?')[1];
const params = query.split('&');
for(let i=0; i < params.length; i++){
const val = params[i].split('=');
if('id' == val[0]){
horse_id = val[1];
}
}
}
$.get('https://db.netkeiba.com/ranking/ajax_horse_bbs_ranking.html',
{
input: 'UTF-8',
output: 'json',
id: horse_id,
},
function(data){
if('OK' == data.status){
$('#horse_bbs_ranking_box').html(data.data);
}
});
});
</script>

